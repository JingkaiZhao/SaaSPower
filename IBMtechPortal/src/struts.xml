<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="global_pages" namespace="/"
		extends="struts-default">
		<action name="ErrorPage" class="edu.tongji.sse.ibm.action.ErrorPageAction">
			<result name="success" type="dispatcher">
				/errorPage.jsp
			</result>
		</action>
	</package>
	<package name="global_service" extends="struts-default" namespace="/">
		<action name="logout"
			class="edu.tongji.sse.ibm.action.LogoutAction">
			<result name="success" type="dispatcher">
				/test/test_success.jsp
			</result>
		</action>
	</package>
	<include file="cdug_actions.xml"></include>
	<include file="portal_actions.xml"></include>
</struts>    
