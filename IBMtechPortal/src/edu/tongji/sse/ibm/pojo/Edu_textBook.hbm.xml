<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.tongji.sse.ibm.pojo">
	<class name="Edu_textBook" lazy="false">
		<id name="id" type="java.lang.Integer">
			<column name="id"></column>
			<generator class="native"></generator>
		</id>
		<property name="name" type="java.lang.String">
			<column name="name"></column>
		</property>
		<property name="mainEditor" type="java.lang.String">
			<column name="mainEditor"></column>
		</property>
		<property name="publishDate" type="java.lang.String">
			<column name="publishDate"></column>
		</property>
		<property name="profile" type="java.lang.String">
			<column name="profile" sql-type="TEXT"></column>
		</property>
		<property name="picURL" type="java.lang.String">
			<column name="picURL"></column>
		</property>
	</class>
</hibernate-mapping>